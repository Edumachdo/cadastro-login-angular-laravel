# Configurações do ambiente Laravel

# Nome da aplicação
APP_NAME=Laravel

# Ambiente da aplicação (local, production, etc)
APP_ENV=local

# Chave da aplicação (gerar com php artisan key:generate)
APP_KEY=base64:YOUR_APP_KEY_HERE

# Habilita modo debug (true ou false)
APP_DEBUG=true

# URL base da aplicação
APP_URL=http://localhost

# Configurações de log
LOG_CHANNEL=stack
LOG_LEVEL=debug

# Configuração do banco de dados MySQL
DB_CONNECTION=mysql
DB_HOST=127.0.0.1
DB_PORT=3306
DB_DATABASE=registro_teste
DB_USERNAME=root
DB_PASSWORD=root

# Configurações do Laravel Sanctum para autenticação
SANCTUM_STATEFUL_DOMAINS=localhost
SESSION_DOMAIN=localhost

# Configurações de cache, sessão e fila padrão
BROADCAST_DRIVER=log
CACHE_DRIVER=file
FILESYSTEM_DRIVER=local
QUEUE_CONNECTION=sync
SESSION_DRIVER=file
SESSION_LIFETIME=120
